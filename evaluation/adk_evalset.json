{
  "eval_set_id": "htcondor_mcp_adk_eval_v1",
  "eval_set_name": "HTCondor MCP Agent ADK Evaluation Set",
  "description": "Comprehensive evaluation set for HTCondor MCP agent covering job listing, status queries, job submission, advanced job information, cluster/pool information, resource monitoring, and reporting/analytics on ATLAS AF.",
  "version": "1.0.0",
  "eval_cases": [
    {
      "name": "Basic Job Listing",
      "description": "Test basic job listing functionality without filters",
      "data": [
        {
          "query": "Show me all jobs",
          "expected_tool_use": [
            {
              "tool_name": "list_jobs",
              "tool_input": {
                "owner": null,
                "status": null,
                "limit": 10
              }
            }
          ],
          "expected_response_substrings": [
            "Jobs in the queue",
            "ClusterId",
            "ProcId", 
            "Status",
            "Owner"
          ]
        }
      ]
    },
    {
      "name": "Job Listing by Owner",
      "description": "Test job listing filtered by specific user",
      "data": [
        {
          "query": "Show jobs for user alice",
          "expected_tool_use": [
            {
              "tool_name": "list_jobs",
              "tool_input": {
                "owner": "alice",
                "status": null,
                "limit": 10
              }
            }
          ],
          "expected_response_substrings": [
            "Jobs for user alice",
            "alice"
          ]
        }
      ]
    },
    {
      "name": "Job Listing by Status",
      "description": "Test job listing filtered by job status",
      "data": [
        {
          "query": "Show running jobs",
          "expected_tool_use": [
            {
              "tool_name": "list_jobs",
              "tool_input": {
                "owner": null,
                "status": "running",
                "limit": 10
              }
            }
          ],
          "expected_response_substrings": [
            "Running jobs",
            "Running"
          ]
        }
      ]
    },
    {
      "name": "Job Listing with Combined Filters",
      "description": "Test job listing with both owner and status filters",
      "data": [
        {
          "query": "Show running jobs for user bob",
          "expected_tool_use": [
            {
              "tool_name": "list_jobs",
              "tool_input": {
                "owner": "bob",
                "status": "running",
                "limit": 10
              }
            }
          ],
          "expected_response_substrings": [
            "Jobs for user bob",
            "Running",
            "bob"
          ]
        }
      ]
    },
    {
      "name": "Get Job Status",
      "description": "Test getting detailed status for a specific job",
      "data": [
        {
          "query": "What's the status of job 1234567?",
          "expected_tool_use": [
            {
              "tool_name": "get_job_status",
              "tool_input": {
                "cluster_id": 1234567
              }
            }
          ],
          "expected_response_substrings": [
            "Job 1234567",
            "status"
          ]
        }
      ]
    },
    {
      "name": "Submit Job",
      "description": "Test job submission functionality",
      "data": [
        {
          "query": "Submit a job to run /bin/sleep 100",
          "expected_tool_use": [
            {
              "tool_name": "submit_job",
              "tool_input": {
                "submit_description": {
                  "executable": "/bin/sleep",
                  "arguments": "100"
                }
              }
            }
          ],
          "expected_response_substrings": [
            "Job submitted",
            "ClusterId"
          ]
        }
      ]
    },
    {
